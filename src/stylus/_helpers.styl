/*List helper*/
.list
  &.v-menu-list
    padding: 15px
    border-radius: 4px
    div
      margin: 3px 0
      &.active-state
        a
          color: rgba(0,0,0,0.87)
          background: $theme.primary !important
          .v-list__tile__title
            color: rgba(255, 255, 255, 0.87)
          i
            color: #FFF
      a
        color: rgba(0,0,0,0.87)
        border-radius: 4px
        .v-list__tile__title
          ele-font()
          font-size: 15px
        .compact-action
          min-width: 35px
        &:hover
          background: $theme.primary !important
          .v-list__tile__title
            color: rgba(255, 255, 255, 0.87)
          i
            color: #FFF
          .menu-list-hover-text
            color: #FFF
